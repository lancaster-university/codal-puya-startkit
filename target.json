{
    "architecture": "CORTEX_M0",
    "asm_flags": "-fno-exceptions -fno-unwind-tables --specs=nosys.specs -mcpu=cortex-m0plus -mthumb",
    "c_flags": "-std=c99 --specs=nosys.specs -Warray-bounds",
    "cmake_definitions": {
        "MBED_LEGACY_TOOLCHAIN": "GCC_ARM;"
    },
    "config": {
        "CAPTOUCH_DEFAULT_CALIBRATION": 3500,
        "CODAL_DEBUG": "CODAL_DEBUG_DISABLED",
        "CODAL_PROVIDE_PRINTF": 1,
        "CODAL_TIMER_32BIT": 1,
        "CODAL_TIMESTAMP": "uint32_t",
        "CODAL_VERSION": "\"unknown\"",
        "CONFIG_GPIO_AS_PINRESET": 1,
        "DEVICE_COMPONENT_COUNT": 10,
        "DEVICE_DEFAULT_PULLMODE": "PullMode::None",
        "DEVICE_DEFAULT_SERIAL_MODE": "SYNC_SLEEP",
        "DEVICE_HEAP_ALLOCATOR": 1,
        "DEVICE_I2C_IRQ_SHARED": 1,
        "DEVICE_PANIC_HEAP_FULL": 1,
        "DEVICE_SRAM_BASE": "0x20000000",
        "DEVICE_SRAM_END": "0x20000C00",
        "DEVICE_STACK_BASE": "DEVICE_SRAM_END",
        "DEVICE_STACK_SIZE": 128,
        "DEVICE_TAG": 0,
        "DEVICE_USB": 0,
        "DMESG_SERIAL_DEBUG": 0,
        "EVENT_LISTENER_DEFAULT_FLAGS": "MESSAGE_BUS_LISTENER_QUEUE_IF_BUSY",
        "MESSAGE_BUS_LISTENER_MAX_QUEUE_DEPTH": 4,
        "PROCESSOR_WORD_TYPE": "uint32_t",
        "SCHEDULER_TICK_PERIOD_US": 4000,
    },
    "cpp_flags": "-std=c++11 -fwrapv -fno-rtti -fno-threadsafe-statics -fno-exceptions -fno-unwind-tables -Wl,--gc-sections -Wl,--sort-common -Wl,--sort-section=alignment -Wno-array-bounds",
    "cpu_opts": "-mcpu=cortex-m0plus -mthumb -mfpu=fpv4-sp-d16 -mfloat-abi=softfp",
    "definitions": "-DPY32F002Bx5 -D__CORTEX_M0PLUS -DTOOLCHAIN_GCC -D__START=target_start",
    "device": "PUYA",
    "generate_bin": true,
    "generate_hex": true,
    "libraries": [
        {
            "branch": "master",
            "name": "codal-core",
            "type": "git",
            "url": "https://github.com/lancaster-university/codal-core"
        },
        {
            "branch": "master",
            "name": "codal-py32",
            "type": "git",
            "url": "https://github.com/lancaster-university/codal-py32"
        }
    ],
    "linker_flags": "-Wl,--no-wchar-size-warning -Wl,--gc-sections -Wl,--wrap,atexit -mcpu=cortex-m0plus -mthumb -mfpu=fpv4-sp-d16 -mfloat-abi=softfp -Wl,--start-group -lstdc++ -lsupc++ -lm -lc -lgcc -lnosys -Wl,--end-group",
    "post_process": "",
    "processor": "py32f002bxx",
    "show_size": true,
    "snapshot_version": "v0.0.1",
    "toolchain": "ARM_GCC"
}